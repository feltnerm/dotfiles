"amixer set Master 5+ unmute"
 XF86AudioRaiseVolume

"amixer set Master 5- unmute"
 XF86AudioLowerVolume

"amixer set 'Master' toggle"
 XF86AudioMute

#"audioplayer -next"
# XF86AudioNext

#"audioplayer -pause"
# XF86AudioPlay

#"audioplayer -stop"
# XF86AudioStop

#"audioplayer -previous"
# XF86AudioPrev